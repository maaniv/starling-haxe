<?xml version="1.0" encoding="utf-8"?>
<project>
	<meta title="Starling Demo" package="com.maaniv" version="0.0.1" company="none"/>
	<app main="Main" file="starling-demo" path="bin" swf-version="17"/>

	<window width="320" height="480" orientation="portrait" background="0xffffff"/>
	<window resizable="false" unless="mobile"/>
	<window fps="60" unless="html5"/>

	<source path="src"/>

	<haxelib name="openfl"/>
	<haxelib name="starling-haxe"/>

	<assets path="assets/audio/wing_flap.mp3" if="web" />
	<assets path="assets/audio/wing_flap.ogg" unless="flash" />
	<assets path="assets/textures/1x" />
	<assets path="assets/fonts/1x" />
	<assets path="assets/fonts/Ubuntu-R.ttf" />
</project>
